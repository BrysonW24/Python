Image = final_generate_links_df["Experiment Image"]
Card_type = "BackgroundImagePrimaryAction"
AEM_BIDS = final_generate_links_df["BID_Links"]
AEM_links = final_generate_links_df["AEM_Links"]
Imagery_path = "https://author.digital.aem.cba/siteadmin#/content/dam/commbank-assets/app/" + final_generate_links_df["Milestone"]
Imagery_live = "https://www.commbank.com.au/content/dam/commbank-assets/app/hero-banner/X15/" + final_generate_links_df["Experiment Image"]
AEM_editor_draft = "https://author.digital.aem.cba/editor.html/content/ext/ActivityFeed/Draft/CA-CEE/" + final_generate_links_df["BID_Links"] + ".html"
AEM_editor_prod = "https://author.digital.aem.cba/editor.html/content/ext/ActivityFeed/LiveCopy/CA-CEE/" + final_generate_links_df["BID_Links"] + ".html"
